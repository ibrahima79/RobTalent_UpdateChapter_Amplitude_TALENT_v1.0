    <ui:InvokeCode ContinueOnError="{x:Null}" Code="' Définir les colonnes à garder avec leur index et valeur par défaut&#xD;&#xA;Dim columnsInfo = New List(Of Tuple(Of String, Integer, String)) From {&#xD;&#xA;    Tuple.Create(&quot;STATUT&quot;, 0, &quot;A traiter&quot;),&#xD;&#xA;    Tuple.Create(&quot;MESSAGE&quot;, 1, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Clé&quot;, 2, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Motif du changement&quot;, 3, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Type Client&quot;, 4, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Nom&quot;, 5, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Prénom&quot;, 6, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Code client&quot;, 7, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Agence&quot;, 8, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Devise&quot;, 9, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Code agent economique&quot;, 10, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Modification des attributs&quot;, 11, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Ancien chapitre&quot;, 12, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Nouveau chapitre&quot;, 13, &quot;&quot;),&#xD;&#xA;&#x9;Tuple.Create(&quot;Code produit&quot;, 14, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Création&quot;, 15, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Mise à jour&quot;, 16, &quot;&quot;),&#xD;&#xA;    Tuple.Create(&quot;Etat Amplitude&quot;, 17, &quot;False&quot;),&#xD;&#xA;    Tuple.Create(&quot;Etat Workflow&quot;, 18, &quot;False&quot;),&#xD;&#xA;    Tuple.Create(&quot;Etat Capture&quot;, 20, &quot;False&quot;)&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;' Supprimer les colonnes non listées&#xD;&#xA;For i As Integer = dtExtractFormatted.Columns.Count - 1 To 0 Step -1&#xD;&#xA;    Dim colName = dtExtractFormatted.Columns(i).ColumnName&#xD;&#xA;    If Not columnsInfo.Any(Function(c) c.Item1 = colName) Then&#xD;&#xA;        dtExtractFormatted.Columns.RemoveAt(i)&#xD;&#xA;    End If&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;' Ajouter ou repositionner les colonnes&#xD;&#xA;For Each colInfo In columnsInfo&#xD;&#xA;    Dim colName = colInfo.Item1&#xD;&#xA;    Dim colIndex = colInfo.Item2&#xD;&#xA;    Dim defaultValue = colInfo.Item3&#xD;&#xA;&#xD;&#xA;    ' Ajouter la colonne si elle n'existe pas&#xD;&#xA;    If Not dtExtractFormatted.Columns.Contains(colName) Then&#xD;&#xA;        Dim newCol As New DataColumn(colName, GetType(String))&#xD;&#xA;        If Not String.IsNullOrEmpty(defaultValue) Then&#xD;&#xA;            newCol.DefaultValue = defaultValue&#xD;&#xA;        End If&#xD;&#xA;        dtExtractFormatted.Columns.Add(newCol)&#xD;&#xA;    End If&#xD;&#xA;&#xD;&#xA;    ' Protéger contre les SetOrdinal() invalides&#xD;&#xA;    If colIndex &lt; dtExtractFormatted.Columns.Count Then&#xD;&#xA;        dtExtractFormatted.Columns(colName).SetOrdinal(colIndex)&#xD;&#xA;    Else&#xD;&#xA;        ' En dernier recours : placer la colonne à la fin&#xD;&#xA;        dtExtractFormatted.Columns(colName).SetOrdinal(dtExtractFormatted.Columns.Count - 1)&#xD;&#xA;    End If&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;' Supprimer les lignes dont la colonne &quot;Clé&quot; est vide ou null&#xD;&#xA;For i As Integer = dtExtractFormatted.Rows.Count - 1 To 0 Step -1&#xD;&#xA;    Dim cle = dtExtractFormatted.Rows(i)(&quot;Clé&quot;)&#xD;&#xA;    If cle Is DBNull.Value OrElse String.IsNullOrWhiteSpace(cle.ToString) Then&#xD;&#xA;        dtExtractFormatted.Rows.RemoveAt(i)&#xD;&#xA;    End If&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="433.6,196.8" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
